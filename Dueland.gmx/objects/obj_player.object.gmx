            <string>/// Player Movements
var rkey = keyboard_check(ord("D"));
var lkey = keyboard_check(ord("A"));
var jkey = keyboard_check(vk_space);

//Check for ground
if place_meeting(x, y+1, par_blocks) {
   // Jumping
   if (jkey) {
       vspeed = -jspd;
   }
}else{
    // Gravity
    if (vspeed &lt; 10) {
        vspeed += grav;
    }
}

// Moving right
if (rkey) {
    hspeed = spd; 
}

// Moving left
if (lkey) {
    hspeed = -spd; 
}

// Check for not moving
if ((!rkey &amp;&amp; !lkey) || (rkey &amp;&amp; lkey)) {
    hspeed = 0;
}

//Control the sprites
if (vspeed != 0 or !place_meeting(x,y+1,par_blocks)) {
    sprite_index = spr_player_jump;
    image_speed = 0;
    image_index = vspeed &gt; 0;
}else{
    if (hspeed != 0) {
        sprite_index = spr_man_walk;
        image_speed = .3;
    }else{
        sprite_index = spr_player_stand;
    }
}

//Control player's facing direction
if (hspeed &gt; 0) {
    image_xscale = 1;
}else if(hspeed &lt; 0) {
    image_xscale = -1;
}
</string>
